/*插入排序java*/
public int[] sortArray(int[] nums) {
        int n=nums.length;
         int tmp;
         int j=0;
         for(int i=1;i<n;i++)
         {
             tmp=nums[i];
             for(j=i;j>0&&nums[j-1]>tmp;j--)
             {
                 nums[j]=nums[j-1];
                // nums[j-1]=tmp;  
             }
             nums[j]=tmp;
         }
         return nums;
    }
